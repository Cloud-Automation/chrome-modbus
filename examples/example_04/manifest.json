{
    "name": "Chrome Modbus Server",
    "description": "Development Tool for Chrome Automation / Modbus Server",
    "version": "0.1",
    "permissions": [ 
        "storage"
    ],
    "app": {
        "background": {
            "scripts": [ "jquery.min.js", "modbus.min.js", "background.js" ]
        }
    },
    "sockets": {
        "tcpServer": {
            "listen":"*:8001"
        }
    }
}
